{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "react-css-modules",
      {
        "generateScopedName": "[name]__[local]:[hash:base64:5]",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        },
        "webpackHotModuleReloading": true
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
